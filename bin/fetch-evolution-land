#!/usr/bin/env node

const path = require('path');
const { fetchEvolutionLandSnapshot } = require('../src/fetch-evolution-land');

async function main() {
  const outputDir = path.join(__dirname, '..', 'data');
  try {
    await fetchEvolutionLandSnapshot(outputDir);
  } catch (error) {
    console.error('‚ùå Fatal error:', error.message);
    process.exit(1);
  }
}

main();
