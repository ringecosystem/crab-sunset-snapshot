#!/usr/bin/env node

const path = require('path');
const { writeXringXwringLpProviders } = require('../src/special/xring-xwring-lp-providers');

async function main() {
	const outputDir = path.join(__dirname, '..', 'data');

	try {
		writeXringXwringLpProviders(outputDir);
	} catch (error) {
		console.error('‚ùå Fatal error:', error.message);
		process.exit(1);
	}
}

main();
