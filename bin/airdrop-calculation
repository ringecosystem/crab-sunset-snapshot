#!/usr/bin/env node

const path = require('path');
const { calculateAirdrop } = require('../src/special/airdrop-calculation');

async function main() {
	const outputDir = path.join(__dirname, '..', 'data');

	try {
		await calculateAirdrop(outputDir);
	} catch (error) {
		console.error('‚ùå Fatal error:', error.message);
		process.exit(1);
	}
}

main();
